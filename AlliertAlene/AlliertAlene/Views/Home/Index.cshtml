@section SPAViews {
    @Html.Partial("_Home")
}
@section Scripts{
    @Scripts.Render("~/bundles/app")
<script>
    (function ($) {
        $(window).load(function () {
            $("#infoBox").mCustomScrollbar();
            $(".pop-img").magnificPopup({ type: 'image' });
            $('.timeline-carousel').slick({
                infinite: true,
                slidesToShow: 4,
                slidesToScroll: 3
            });
        });
    })(jQuery);
</script>
}